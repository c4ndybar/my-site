    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/config": "^3.3.4",
    "@types/firebase": "^3.2.1",
    "@types/spotify-web-api-node": "^5.0.11",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "private": true
}
